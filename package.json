  "engines": {
    "node": ">=5.10"
  },
  "dependencies": {
    "bowser": "^1.8.1",
    "he": "^1.1.1",
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.2",
    "babel-jest": "^23.4.2",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-replace": "^1.0.0",
    "gulp-typescript": "^4.0.2",
    "jest": "^23.5.0",
    "standard-version": "^4.4.0",
    "ts-jest": "^23.1.4",
    "tslint": "^5.11.0",
    "tslint-config-semistandard": "^7.0.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
  },
  "keywords": [
    "filestack",
    "filepicker",
    "upload",
    "files",
    "multipart",
    "S3",
